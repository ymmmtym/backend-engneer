site_name: Engneer Tutorial
nav:
  - Home: index.md
theme:
  feature:
    tabs: true
  language: en
  name: material
  palette:
    accent: Ocean
    primary: Purple
markdown_extensions:
  - meta
  - pymdownx.mark
  - pymdownx.magiclink
  - pymdownx.emoji
  - pymdownx.keys
  - def_list
  - admonition
  - codehilite:
      linenums: true
      use_pygments: true